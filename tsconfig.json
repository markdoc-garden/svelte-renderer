{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,

    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "skipLibCheck": true,
    "sourceMap": true,

    "strict": true,
    "noImplicitReturns": true,
    "useUnknownInCatchVariables": true
  }
}
